.PHONY: r0 r1 r2

.PHONY: all

all:  r0 r1 r2

r0:
	./BitTorrentSingleProcessor -f ../simulations/MultiplePeers.ini -c MultiplePeers -u Cmdenv -r 0 > ./logs/out_0.log 2> ./logs/err_0.log
r1:
	./BitTorrentSingleProcessor -f ../simulations/MultiplePeers.ini -c MultiplePeers -u Cmdenv -r 1 > ./logs/out_1.log 2> ./logs/err_1.log
r2:
	./BitTorrentSingleProcessor -f ../simulations/MultiplePeers.ini -c MultiplePeers -u Cmdenv -r 2 > ./logs/out_2.log 2> ./logs/err_2.log
